algoritmo "semnome"
var
   c_dia, anos, dias_perd: real
inicio
      Escreval ("Quantos cigarros você fuma por dia? ")
      leia (c_dia)
      Escreval ("Quantos anos fumando? ")
      leia (anos)
      hora_dia <- ((c_dia * 10) / 60)
      
      Escreva (dias_perd, "dias")
fimalgoritmo
